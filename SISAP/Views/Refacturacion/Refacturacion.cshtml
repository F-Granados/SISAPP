
@{
    ViewBag.Title = "Refacturacion";
}

<div class="card border-0 ml-4 mt-4 margin-1 mb-3">
    <div class="card-body fuente">
        <div class="card-header bg-dark text-white">
            <h5><i class="fas fa-table"> Refacturacion</i> </h5>
        </div>
        <hr  style="border-style: dashed"/>
        <div class="row">
            <div class="col-12">
                <div class="d-flex justify-content-end mb-2">
                    <div class="searchbar">
                        <input type="text" name="name" class="search_input" value=""  placeholder="Buscar......"/>
                        <a href="#" class="search_icon"> <i class="fas fa-search"></i> </a>

                    </div>

                </div>

            </div>

        </div>
        <div class="table-responsive-lg">
            <table class="table table-sm table-striped" id="allGrilla">
                <thead>
                    <tr class="bg-info text-light">
                        <th scope="col">Año</th>
                        <th scope="col">Mes</th>
                        <th scope="col">Lectura Antigua</th>
                        <th scope="col">Lectura Actual</th>
                        <th scope="col">Consumo</th>
                        <th scope="col">Opcion</th>

                    </tr>
                </thead>
       
            </table>

        </div>
    </div>

</div>


